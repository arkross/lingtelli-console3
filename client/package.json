{
  "name": "bot-console",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "chart.js": "^2.9.4",
    "d3": "^5.4.0",
    "github-markdown-css": "^3.0.1",
    "highlight.js": "^9.15.6",
    "i18next": "^10.3.0",
    "i18next-browser-languagedetector": "^2.1.0",
    "i18next-xhr-backend": "^1.5.1",
    "immutable": "^3.8.2",
    "lodash": "^4.17.10",
    "moment": "^2.20.1",
    "node-sass": "^4.11.0",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.0",
    "query-string": "^6.2.0",
    "react": "^16.2.0",
    "react-app-env": "^1.2.0",
    "react-c3js": "^0.1.20",
    "react-chartjs-2": "^2.7.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.2.0",
    "react-dropzone": "4.2.7",
    "react-file-download": "^0.3.5",
    "react-i18next": "^7.3.4",
    "react-markdown": "^4.0.6",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^2.0.3",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "semantic-ui-react": "0.81.3",
    "validator": "^9.2.0"
  },
  "scripts": {
    "start-js": "react-app-env --env-file=env/development.env start",
    "start-js-alone": "react-app-env --env-file=env/standalone.env start",
    "build-js": "react-app-env --env-file=env/production.env build",
    "build-jarvis": "react-app-env --env-file=env/production.jarvis.env build",
    "build-alone": "react-app-env --env-file=env/standalone.env build",
    "jest": "react-app-env --env-file=env/test.env --env=jsdom test",
    "test": "NODE_ENV=test mocha src/test/*.mocha.js",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar --output-style compressed src/styles/style.scss -o public/css",
    "watch-css": "node-sass-chokidar src/styles/style.scss -o public/css/ --source-map true --watch --recursive --usePolling",
    "build-less": "node-less-chokidar node_modules/semantic-ui-less/semantic.less",
    "watch-less": "node-less-chokidar node_modules/semantic-ui-less/semantic.less --watch",
    "start": "npm-run-all -p build-less watch-css start-js",
    "start-alone": "npm-run-all -p watch-css start-js-alone",
    "build": "npm-run-all -s build-css build-less build-js",
    "build:jarvis": "npm-run-all build-css build-less build-jarvis",
    "build:standalone": "npm-run-all build-css build-less build-alone"
  },
  "devDependencies": {
    "babel-polyfill": "^6.26.0",
    "chai": "^4.1.2",
    "dotenv": "^6.0.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "node-less-chokidar": "^0.4.1",
    "react-test-renderer": "^16.4.1",
    "regenerator-runtime": "^0.12.0",
    "semantic-ui-less": "^2.4.1"
  },
  "babel": {
    "presets": [
      "react",
      "env"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
